{
  "name": "css-frameworks-ca",
  "version": "0.0.1",
  "description": "An social media app for css-frameworks-ca",
  "scripts": {
    "test": "echo \"This is my CA delivery in css-frameworks\" && exit 1",
    "build": "sass src/scss:dist/css && npm run copy-icons",
    "dev": "npm-run-all --parallel watch start-server",
    "watch": "sass --watch src/scss:dist/css",
    "start-server": "live-server",
    "copy-icons": "cp node_modules/bootstrap-icons/font/fonts/* dist/fonts/"    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonhavbra87/css-frameworks-ca.git"
  },
  "keywords": [
    "CSS",
    "Frameworks",
    "Bootstrap",
    "SCSS",
    "SASS"
  ],
  "author": "Jon Are Haveråen-Brattås <jonhav53328@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonhavbra87/css-frameworks-ca/issues"
  },
  "homepage": "https://github.com/jonhavbra87/css-frameworks-ca#readme",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.72.0"
  }
}
